  "bugs": {
    "url": "https://github.com/weswigham/solved/issues"
  },
  "homepage": "https://github.com/weswigham/solved#readme",
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/mocha": "^2.2.38",
    "@types/node": "^7.0.4",
    "chai": "^3.5.0",
    "git-pre-commit": "^2.1.3",
    "mocha": "^3.2.0",
    "ts-node": "^2.0.0",
    "typescript": "^2.2.1"
  }
